(define (rect rgb x y height width)
  (setposition x y)
  (setheading 90)
  (pendown)
  (color rgb)
  (begin_fill)
  (forward width)
  (right 90)
  (forward height)
  (right 90)
  (forward width)
  (right 90)
  (forward height)
  (end_fill)
  (penup)
)

(define (gradient colors y rect_size)
  (if (null? colors)
    nil
    (begin
      (rect (car colors) (* -0.5 (screen_width)) y rect_size (screen_width))
      (gradient (cdr colors) (- y rect_size) rect_size)
    )
  )
)

(define (fill_circle rgb radius)
  (color rgb)
  (pendown)
  (begin_fill)
  (circle radius)
  (end_fill)
  (penup)
)

(define (cloud1 rgb x y height width)
  (setposition x y)
  (rect rgb x y height width)
  (setheading 0)
  (setposition (+ x (* 1.2 height)) (+ y (* 0.2 height)))
  (fill_circle rgb (* 1.2 height))
  (setposition (+ x (* 0.9 width)) (+ y (* 0.5 height)))
  (fill_circle rgb (* height 1.5))
  (setposition (+ x width (* 0.8 height)) (- y (* 0.2 height)))
  (fill_circle rgb (* height 0.8))
)

(define (cloud2 rgb x y height width)
  (setposition x y)
  (rect rgb x y height width)
  (setheading 0)
  (setposition (+ x (* 0.7 height)) (- y (* 0.3 height)))
  (fill_circle rgb (* height 0.7))
  (setposition (+ x (* 0.53 width)) (+ y (* 0.05 height)))
  (fill_circle rgb (* height 1.05))
  (setposition (+ x (* 0.9 width)) (+ y (* 0.3 height)))
  (fill_circle rgb (* height 1.3))
  (setposition (+ x width height) y)
  (fill_circle rgb height)
)

(define (draw_clouds rgb)
  (cloud2 rgb (* -0.1 (screen_width)) (* 0.2 (screen_height)) 40 150)
  (cloud2 rgb (* -0.4 (screen_width)) (* -0.3 (screen_height)) 60 210)
  (cloud1 rgb (* -0.45 (screen_width)) (* 0.3 (screen_height)) 30 110)
  (cloud1 rgb (* 0.2 (screen_width)) (* -0.4 (screen_height)) 70 240)
  (cloud1 rgb (* 0.37 (screen_width)) (* 0.05 (screen_height)) 35 140)
  (cloud1 rgb (* 0.3 (screen_width)) (* 0.4 (screen_height)) 25 90)
)

(define (draw_pixel_row row x y size)
  (if (null? row)
    nil
    (begin
      (define pixels 1)
      (if (integer? (car row))
        (define pixels (car row))
        (pixel x y (car row))
      )
      (draw_pixel_row (cdr row) x (+ y (* size pixels)) size)
    )
  )
)

(define (draw_pixels matrix x y size)
  (if (null? matrix)
    nil
    (begin
      (draw_pixel_row (car matrix) x y size)
      (draw_pixels (cdr matrix) (+ x size) y size)
    )
  )
)

(define (draw_line rgb x1 y1 x2 y2)
  (penup)
  (color rgb)
  (setposition x1 y1)
  (begin_fill)
  (pendown)
  (setposition x2 y2)
  (setposition x2 (+ y2 3))
  (setposition x1 (+ y1 3))
  (end_fill)
  (penup)
)

